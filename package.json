{
  "name": "@daopk/eslint-config",
  "type": "module",
  "version": "0.5.2",
  "license": "ISC",
  "repository": "github:daopk/eslint-config",
  "exports": {
    ".": "./dist/index.mjs",
    "./nestjs": {
      "import": "./dist/presets/nestjs.mjs",
      "types": "./dist/presets/nestjs.d.mts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "eslint-config-inspector",
    "typegen": "tsx scripts/typegen.ts",
    "build": "pnpm typegen && tsdown",
    "build:inspector": "pnpm build && eslint-config-inspector build",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "local-pkg": "^1.1.2",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.3.0",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-typegen": "^2.3.0",
    "jiti": "^2.6.1",
    "tsdown": "^0.16.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
